{
  "name": "ixananary",
  "description": "Next.js, TypeScript, TailwindCSS",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:css": "postcss src/style/tailwind.css -o src/style/bundle.css",
    "dev:next": "next",
    "build": "run-s build:*",
    "build:css": "NODE_ENV=production postcss src/style/tailwind.css -o src/style/bundle.css",
    "build:next": "next build",
    "start": "next start"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "next": "^10.0.8-canary.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "server": "1.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@types/node": "^14.14.22",
    "@types/puppeteer": "^5.4.3",
    "@types/react": "^17.0.0",
    "autoprefixer": "10.2.3",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0",
    "postcss-preset-env": "^6.7.0",
    "puppeteer": "^7.0.1",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3",
    "webpack": "^5.20.1"
