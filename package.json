{
  "name": "ixanary",
  "description": "Next.js, TypeScript, AMP, TailwindCSS",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next",
    "dev:css": "postcss src/style/index.css --config postcss.config.js -o src/style/tailwind.css -w",
    "build": "build:css && next build",
    "build:css": "postcss src/style/index.css --config postcss.config.js -o src/style/bundle.css",
    "start": "next start"
  },
  "dependencies": {
    "next": "^10.0.6-canary.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "autoprefixer": "10.2.3",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  },
  "license": "MIT"
}
